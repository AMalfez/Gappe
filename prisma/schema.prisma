generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  bio       String
  image     String
  userId    String @unique
  name      String
  username  String
  onboarded Boolean @default(false)
}
